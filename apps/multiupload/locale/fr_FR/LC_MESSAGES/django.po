#: apps/multiupload/apps.py:15
msgid "Multi-upload form field"
msgstr "Champ de formulaire multi-fichiers"

#: apps/multiupload/fields.py:56
#, python-format
msgid ""
"Ensure that at least %(min_num)s files are uploaded (received %(num_files)s)."
msgstr ""
"Assurez-vous qu'au moins %(min_num)s fichiers sont envoyés (reçu "
"%(num_files)s fichier(s))."

#: apps/multiupload/fields.py:59
#, python-format
msgid ""
"Ensure that at most %(max_num)s files are uploaded (received %(num_files)s)."
msgstr ""
"Assurez-vous qu'au plus %(max_num)s fichiers sont envoyés (reçu "
"%(num_files)s fichier(s))."

#: apps/multiupload/fields.py:66
#, python-format
msgid ""
"File \"%(uploaded_file_name)s\" exceeded maximum upload size %(max_size)s."
msgstr ""
"Le fichier \"%(uploaded_file_name)s\" excède la taille maximum de "
"%(max_size)s. "

#: apps/multiupload/fields.py:71
#, python-format
msgid "Total files size exceeded maximum upload size %(total_max_size)s."
msgstr ""
"La taille totale des fichiers excède la taille maximum de %(total_max_size)s."

#: apps/multiupload/fields.py:78
#, python-format
msgid "%dB"
msgstr "%do"

#: apps/multiupload/fields.py:80
#, python-format
msgid "%dKB"
msgstr "%dKo"

#: apps/multiupload/fields.py:82
#, python-format
msgid "%.2fMB"
msgstr "%.2fMo"
